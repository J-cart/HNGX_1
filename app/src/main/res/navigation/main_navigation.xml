<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation.xml"
    app:startDestination="@id/profile">

    <fragment
        android:id="@+id/profile"
        android:name="com.tutorials.hngx1.Profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profile_to_githubWebView"
            app:destination="@id/githubWebView" />
    </fragment>
    <fragment
        android:id="@+id/githubWebView"
        android:name="com.tutorials.hngx1.GithubWebView"
        android:label="fragment_github_web_view"
        tools:layout="@layout/fragment_github_web_view" />
</navigation>